{
    "manifestVersion": 1,
    "id": "sprint-goal",
    "name": "Sprint Goal",
    "version": "3.0.7",
    "publisher": "keesschollaart",
    "public": true,
    "helpMarkDown": "[More Information](https://github.com/keesschollaart81/vsts-sprint-goal)",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This extension enables you to set a sprint goal",
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "sprint",
        "goal",
        "scrum"
    ],
    "content": {
        "details": {
            "path": "Marketplace.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "screenshots": [
        {
            "path": "images/screenshots-vsts-sprint-goal-3.png"
        },
        {
            "path": "images/screenshots-vsts-sprint-goal-4.png"
        }
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/keesschollaart81/vsts-sprint-goal"
        },
        "support": {
            "uri": "https://github.com/keesschollaart81/vsts-sprint-goal"
        }
    },
    "branding": {
        "color": "rgb(216, 224, 240)",
        "theme": "light"
    },
    "icons": {
        "default": "images/icon.png",
        "large": "images/icon-large.png"
    },
    "files": [
        {
            "path": "src/sprint-goal.html",
            "addressable": true
        },
        {
            "path": "src/widget/widget.html",
            "addressable": true
        },
        {
            "path": "src/widget/widgetconfig.html",
            "addressable": true
        },
        {
            "path": "src/emojipicker",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "node_modules/applicationinsights-js/bundle",
            "addressable": true,
            "packagePath": "lib"
        }, 
        {
            "path": "scripts",
            "addressable": true
        }
    ],
    "baseUri": "<<baseuri>>",
    "contributions": [
        {
            "id": "iteration-backlog-tab",
            "type": "ms.vss-web.tab",
            "description": "Adds a tab to the Iteration Backlog page.",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "name": "Sprint Goal",
                "title": "Sprint Goal Tab",
                "uri": "src/sprint-goal.html",
                "dynamic": true
            }
        },
        {
            "id": "SprintGoalWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                ".SprintGoalWidget.Configuration"
            ],
            "properties": {
                "name": "Sprint Goal Widget",
                "description": "Sprint Goal widget",
                "catalogIconUrl:": "img/CatalogIcon.png",
                "previewImageUrl": "img/preview.png",
                "uri": "src/widget/widget.html",
                "isNameConfigurable": true,
                "supportedSizes": [
                    {
                        "rowSpan": 1,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 1,
                        "columnSpan": 3
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "SprintGoalWidget.Configuration",
            "type": "ms.vss-dashboards-web.widget-configuration",
            "targets": [
                "ms.vss-dashboards-web.widget-configuration"
            ],
            "properties": {
                "name": "SprintGoalWidget Configuration",
                "description": "Configures SprintGoalWidget",
                "uri": "src/widget/widgetconfig.html"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/keesschollaart81/vsts-sprint-goal"
    },
    "scopes": [
        "vso.work"
    ]
}
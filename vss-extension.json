{
    "manifestVersion": 1,
    "id": "sprint-goal-dev",
    "name": "Sprint Goal Dev",
    "version": "4.0.14",
    "publisher": "keesschollaart",
    "public": false,
    "helpMarkDown": "[More Information](https://github.com/keesschollaart81/vsts-sprint-goal)",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This extension enables you to set a sprint goal",
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "sprint",
        "goal",
        "scrum",
        "work",
        "target",
        "iteration",
        "reason",
        "team",
        "focus",
        "Dashboard Widgets"
    ],
    "content": {
        "details": {
            "path": "Marketplace.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "screenshots": [
        {
            "path": "images/dist/widget-screenshot.png"
        },
        {
            "path": "images/dist/screenshots-vsts-sprint-goal-4.png"
        },
        {
            "path": "images/dist/screenshots-vsts-sprint-goal-4.png"
        }
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/keesschollaart81/vsts-sprint-goal"
        },
        "support": {
            "uri": "https://github.com/keesschollaart81/vsts-sprint-goal"
        }
    },
    "branding": {
        "color": "rgb(216, 224, 240)",
        "theme": "light"
    },
    "icons": {
        "default": "images/dist/icon.png",
        "large": "images/dist/icon-large.png"
    },
    "files": [
        {
            "path": "static/",
            "addressable": true,
            "packagePath": "/"
        },
        {
            "path": "dist/sprint-goal",
            "addressable": true,
            "packagePath": "/"
        }, 
        {
            "path": "images/dist",
            "addressable": true,
            "packagePath": "images/dist/"
        }
    ],
    "contributions": [
        {
            "id": "iteration-backlog-tab",
            "type": "ms.vss-web.tab",
            "description": "Adds a tab to the Iteration Backlog page.",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "name": "Sprint Goal",
                "title": "Sprint Goal Tab",
                "uri": "sprint-goal.html",
                "dynamic": true
            }
        },
        {
            "id": "SprintGoalWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                ".SprintGoalWidget.Configuration"
            ],
            "properties": {
                "name": "Sprint Goal Widget",
                "description": "Sprint Goal",
                "catalogIconUrl:": "images/dist/widget.png",
                "previewImageUrl": "images/dist/widget.png",
                "uri": "widget.html",
                "isNameConfigurable": true,
                "supportedSizes": [
                    {
                        "rowSpan": 1,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 1,
                        "columnSpan": 1
                    },
                    {
                        "rowSpan": 1,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 1,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 3
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "SprintGoalWidget.Configuration",
            "type": "ms.vss-dashboards-web.widget-configuration",
            "targets": [
                "ms.vss-dashboards-web.widget-configuration"
            ],
            "properties": {
                "name": "SprintGoalWidget Configuration",
                "description": "Configures SprintGoalWidget",
                "uri": "widgetconfig.html"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/keesschollaart81/vsts-sprint-goal"
    },
    "scopes": [
        "vso.work"
    ]
}